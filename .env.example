# Billed Gemini API Key (takes priority if set, allows 300 concurrent + 1000 requests/min)
# GEMINI_API_KEY_BILLED=your-billed-api-key-here

# Free tier parallel keys (ignored if GEMINI_API_KEY_BILLED is set)
# Each key allows 1 concurrent + 10 requests/min
# GEMINI_API_KEYS_PARALLEL='["key1","key2","key3","key4","key5","key6","key7","key8"]'
GEMINI_API_KEYS_PARALLEL=

# NextAuth Configuration
NEXTAUTH_URL=http://localhost:8933
# Generate your own secret with: openssl rand -base64 32
NEXTAUTH_SECRET=development-secret-change-in-production-use-openssl-rand-base64-32

DATABASE_PATH=./erudate.db

# PayPal Configuration
# Your business name for PayPal invoices
PAYPAL_BUSINESS_NAME=Exammer
# Your PayPal business email address
PAYPAL_BUSINESS_EMAIL=donations@exammer.com
# PayPal API credentials (get from https://developer.paypal.com)
# For sandbox testing, use sandbox credentials
# For production, use live credentials
PAYPAL_CLIENT_ID=your_paypal_client_id_here
PAYPAL_CLIENT_SECRET=your_paypal_client_secret_here
# PayPal mode: 'sandbox' for testing, 'live' for production
PAYPAL_MODE=sandbox